<!DOCTYPE html>
<html lang="en">

<head>
    <title>@ViewData["Title"]</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <!--LINK-->
    <!--LIBS-->
    <link href="~/lib/bootstrap/css/bootstrap.min.css" type="text/css" rel="stylesheet" />
    <link href="~/js/libs/ohsnap/index.css" rel="stylesheet" type="text/css">
    <!--CUSTOM-->
    <link href="~/css/Administrator/index.css" rel="stylesheet" type="text/css">
</head>

<body>
    <!--NOTIFICATION-->
    <div id="ohsnap"></div>

    <div class="container">
        <!--HEADER-->
        <div class="header">
            Administrator Panel
        </div>


        <a asp-controller="Users" asp-action="Index">Index</a>
        <a asp-controller="Users" asp-action="Managers">Manager</a>
        <a asp-controller="Home" asp-action="Logout" asp-area="">Log out</a>

        <!--CONTENT-->
        <div class="content">
            @RenderBody()
        </div>


        <!--FOOTER-->
        <div class="footer">
            ©Copyright The Architects
        </div>
    </div>

    <!--SCRIPTS-->
    <!--LIBS-->
    <script src="~/js/libs/jquery-3.3.1.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/js/libs/ohsnap/ohsnap.min.js"></script>
    <script src="~/js/libs/bootbox/bootbox.min.js"></script>

    <!--CUSTOM-->
    <script src="~/js/areas/index.js"></script>
    @RenderSection("Scripts", required: false)

</body>
</html>
